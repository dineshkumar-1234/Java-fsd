package dataStructure;

public class InsertionSort {
	public static void main(String[] args) {
		int[] arr={51,22,93,14,55,66};
		insertionSort(arr);
		System.out.println("Sorted Array:");
		for(int num:arr){
			System.out.print(num + " ");
			}
		}
	
	public static void insertionSort(int[] arr){
		int n=arr.length;
		for(int i=1;i<n;i++){
			int key=arr[i];
			int j=i-1;
			
			while(j>=0 && arr[j]>key){
				arr[j+1]=arr[j];
				j--;
				}
			arr[j+1]=key;
			}
		}
}
